{
  "name": "win-slidescreen",
  "version": "4.0.0",
  "description": "A plugin for WinnetouJs that adds slide screen functionality to your application. Version 4 with new features and improvements to be used with WinnetouJs 3+.",
  "main": "dist/main.js",
  "scripts": {
    "compile": "npm run wbr && npm run build-types && cp -r source/* dist/",
    "postinstall": "node ./dist/copy-files-postinstall.js",
    "wbr": "node wbr -b -v",
    "build-types": "tsc --emitDeclarationOnly",
    "publish:alpha": "npm publish --tag alpha",
    "publish:beta": "npm publish --tag beta",
    "publish:latest": "npm publish"
  },
  "files": [
    "dist/"
  ],
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pamydev/win-slidescreen.git"
  },
  "keywords": [
    "plugin",
    "javascript",
    "winnetou",
    "winnetoujs"
  ],
  "author": "Pamela Sedrez <pamela@cedros.dev> (https://github.com/pamydev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pamydev/win-slidescreen/issues"
  },
  "homepage": "https://github.com/pamydev/win-slidescreen#readme",
  "dependencies": {
    "ncp": "^2.0.0",
    "winnetoujs": "^3.1.6"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
